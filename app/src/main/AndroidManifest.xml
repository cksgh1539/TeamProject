<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.teamproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".StudentRestaurant">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity android:name=".SR_noodle"
            android:parentActivityName=".StudentRestaurant">
        </activity>

        <activity android:name=".SR_rice"
            android:parentActivityName=".StudentRestaurant">
        </activity>

        <activity android:name=".SR_fry"
            android:parentActivityName=".StudentRestaurant">
        </activity>

        <activity android:name=".FoodDetail"
            android:parentActivityName=".StudentRestaurant">
        </activity>

        <uses-permission android:name="android.permission.CALL_PHONE"/>


    </application>
</manifest>